(block execmod_all
    (blockinherit bool_object)

    (context execmod_all (sys.u sys.r bool_object
        (systemlow systemlow)))
    (genfscon "selinuxfs" "/booleans/execmod_all"
        execmod_all)
)

(block boot_object
    (blockabstract boot_object)
        (type boot_object)
        (call file_boot_type (boot_object))
)

