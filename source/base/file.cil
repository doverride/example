(filecon "/usr" dir usr)

(type var)
(call file_var_type (var))
(call sys_mountpoint_type (var))

(context var (sys.u sys.r var (systemlow systemlow)))
(filecon "/srv/.*" any var)
(filecon "/srv" dir var)
(filecon "/var/.*" any var)
(filecon "/var" dir var)

(type var_cache)
