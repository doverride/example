(macro file_execmod_all ((type ARG1))
    (booleanif execmod_all
        (true
            (allow ARG1 file_type (file (execmod))))))

(macro file_execmod_all_uncond ((type ARG1))
    (allow ARG1 file_type (file (execmod))))

(macro file_unconfined ((type ARG1))
    (call file_execmod_all (ARG1))
    (typeattributeset file_unconfined_type ARG1))
